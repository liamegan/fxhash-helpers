{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs",
    "private": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "sort": false,
    "theme": "slate",
    "navType": "inline",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
